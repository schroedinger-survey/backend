{
  "name": "schroedinger-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-dev": "NODE_ENV=dev nodemon server.js",
    "start-prod": "NODE_ENV=production node server.js",
    "migrate-prod": "NODE_ENV=production node scripts/migrate.js",
    "migrate-dev": "NODE_ENV=dev node scripts/migrate.js",
    "migrate-test": "NODE_ENV=test node scripts/migrate.js",
    "test-dev": "NODE_ENV=dev jest ./tests/**/*.test.js --coverage --detectOpenHandles",
    "test-test": "NODE_ENV=test jest ./tests/**/*.test.js --coverage --detectOpenHandles",
    "lint": "eslint *.js ./src/**/*.js ./tests/**/*.test.js",
    "fix": "eslint --fix *.js ./src/**/*.js ./tests/**/*.test.js"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.1",
    "express-winston": "^4.0.4",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "redis": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.0.0",
    "winston-elasticsearch": "^0.8.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "@jest/globals": "^26.4.1",
    "supertest": "^4.0.2",
    "eslint": "^7.7.0",
    "jest": "^26.4.0"
  },
  "jest": {
    "verbose": true,
    "testTimeout": 20000
  }
}
