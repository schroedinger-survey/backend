{
  "swagger": "2.0",
  "info": {
    "description": "Schroedinger's REST API documentation",
    "version": "1.0.0",
    "title": "Schroedinger API",
    "contact": {
      "email": "you@schroedinger.de"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "tags": [
    {
      "name": "user"
    },
    {
      "name": "survey"
    },
    {
      "name": "submission"
    },
    {
      "name": "token"
    }
  ],
  "paths": {
    "/user": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "create a new user account",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "recaptcha_token",
            "type": "string",
            "required": true
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/UserRegistration"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User created"
          },
          "400": {
            "description": "User could not be created"
          },
          "409": {
            "description": "Username and/or email allready exist"
          }
        }
      },
      "put": {
        "tags": [
          "user"
        ],
        "summary": "change user credentials (username, email, password)",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ChangeUserData"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "User Data was updated"
          },
          "400": {
            "description": "User Data could not be changed"
          }
        }
      },
      "delete": {
        "tags": [
          "user"
        ],
        "description": "username or email, password required",
        "summary": "user can delete his account permanently",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/DeleteUser"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User account was deleted"
          },
          "400": {
            "description": "User account could not be deleted"
          }
        }
      }
    },
    "/user/login": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "user login with given credentials",
        "parameters": [
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/UserLogin"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User login ok",
            "schema": {
              "$ref": "#/definitions/ExampleToken"
            }
          },
          "403": {
            "description": "Wrong credentials"
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/user/logout": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "user logout from his account",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "204": {
            "description": "successfully logged out, banned token"
          }
        }
      }
    },
    "/user/info": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "checks if user and user jwt-token are valid",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "User and jwt-token are valid",
            "schema": {
              "$ref": "#/definitions/User"
            }
          },
          "403": {
            "description": "User not valid, authorization is missing"
          },
          "404": {
            "description": "User belonging to token not found"
          }
        }
      }
    },
    "/user/password/reset": {
      "post": {
        "description": "If only one credential (username or email) is given this is taken as default. But if both are given email is used to reset password.",
        "tags": [
          "user"
        ],
        "summary": "send link to reset password if forgotten and no login possible",
        "parameters": [
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ResetPasswordRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Link to reset Password was send"
          },
          "403": {
            "description": "Password can not be reset, given Credentials are wrong"
          }
        }
      },
      "put": {
        "tags": [
          "user"
        ],
        "summary": "change the password of the user",
        "parameters": [
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ResetPassword"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Password was changed"
          },
          "400": {
            "description": "Password could not be reset"
          }
        }
      }
    },
    "/survey": {
      "post": {
        "tags": [
          "survey"
        ],
        "summary": "user creates a new survey",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CreateSurvey"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Survey was created",
            "schema": {
              "$ref": "#/definitions/CreateSurvey"
            }
          },
          "400": {
            "description": "could not be created"
          }
        }
      }
    },
    "/survey/{survey_id}": {
      "put": {
        "tags": [
          "survey"
        ],
        "description": "Only pending surveys or surveys that are active, but have no submissions, can be changed",
        "summary": "change the survey details",
        "parameters": [
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/UpdateSurvey"
            }
          },
          {
            "in": "path",
            "name": "survey_id",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Survey was updated"
          },
          "400": {
            "description": "Changes were not possible"
          }
        }
      },
      "delete": {
        "tags": [
          "survey"
        ],
        "summary": "user can delete a survey permanently",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "path",
            "name": "survey_id",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Survey was successfully deleted"
          },
          "400": {
            "description": "Survey could not be deleted"
          },
          "403": {
            "description": "No permission to delete survey (e.g. not the owner)"
          }
        }
      }
    },
    "/survey/public": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "retrieves public surveys, based on a search term",
        "parameters": [
          {
            "in": "query",
            "type": "string",
            "name": "user_id"
          },
          {
            "in": "query",
            "type": "string",
            "name": "title"
          },
          {
            "in": "query",
            "type": "string",
            "name": "description"
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_number",
            "default": 0
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_size",
            "default": 5
          },
          {
            "in": "query",
            "type": "string",
            "name": "start_date"
          },
          {
            "in": "query",
            "type": "string",
            "name": "end_date"
          }
        ],
        "responses": {
          "200": {
            "description": "survey matching the search term was found",
            "schema": {
              "items": {
                "$ref": "#/definitions/UserSurvey"
              }
            }
          }
        }
      }
    },
    "/survey/public/{survey_id}": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "a specific survey is fetched",
        "parameters": [
          {
            "in": "path",
            "name": "survey_id",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "requested survey is available",
            "schema": {
              "$ref": "#/definitions/RequestedSurvey"
            }
          },
          "400": {
            "description": "survey not available"
          }
        }
      }
    },
    "/survey/public/count": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "gets the count of all existing surveys",
        "parameters": [
          {
            "in": "query",
            "type": "string",
            "name": "title"
          },
          {
            "in": "query",
            "type": "string",
            "name": "description"
          },
          {
            "in": "query",
            "type": "string",
            "name": "start_date"
          },
          {
            "in": "query",
            "type": "string",
            "name": "end_date"
          }
        ],
        "responses": {
          "200": {
            "description": "count was calculated",
            "schema": {
              "$ref": "#/definitions/Count"
            }
          }
        }
      }
    },
    "/survey/secured": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "all survey belonging to the user are fetched",
        "parameters": [
          {
            "in": "query",
            "type": "string",
            "name": "title"
          },
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "type": "string",
            "name": "description"
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_number",
            "default": 0
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_size",
            "default": 5
          },
          {
            "in": "query",
            "type": "string",
            "name": "start_date"
          },
          {
            "in": "query",
            "type": "string",
            "name": "end_date"
          }
        ],
        "responses": {
          "200": {
            "description": "all surveys belonging to the user",
            "schema": {
              "items": {
                "$ref": "#/definitions/RequestedSurvey"
              }
            }
          }
        }
      }
    },
    "/survey/secured/{survey_id}": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "a specific survey is fetched",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "token",
            "required": true,
            "type": "string"
          },
          {
            "in": "path",
            "name": "survey_id",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "requested survey is available",
            "schema": {
              "$ref": "#/definitions/RequestedSurvey"
            }
          },
          "400": {
            "description": "survey not available"
          }
        }
      }
    },
    "/survey/secured/count": {
      "get": {
        "tags": [
          "survey"
        ],
        "summary": "gets the count of all existing surveys",
        "parameters": [
          {
            "in": "query",
            "type": "string",
            "name": "title"
          },
          {
            "in": "query",
            "type": "string",
            "name": "description"
          },
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "type": "string",
            "name": "start_date"
          },
          {
            "in": "query",
            "type": "string",
            "name": "end_date"
          }
        ],
        "responses": {
          "200": {
            "description": "count was calculated",
            "schema": {
              "$ref": "#/definitions/Count"
            }
          }
        }
      }
    },
    "/submission": {
      "post": {
        "tags": [
          "submission"
        ],
        "summary": "user took a survey and submits it",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Submission"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "submission successfull"
          },
          "400": {
            "description": "wrong parameters or id not found"
          },
          "500": {
            "description": "submission could not be handed in"
          }
        }
      },
      "get": {
        "tags": [
          "submission"
        ],
        "summary": "owner can get submissions to a survey",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "survey_id",
            "type": "string",
            "required": true
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_number",
            "default": 0
          },
          {
            "in": "query",
            "type": "string",
            "name": "page_size",
            "default": 5
          }
        ],
        "responses": {
          "200": {
            "description": "found submissions to survey",
            "schema": {
              "items": {
                "$ref": "#/definitions/Submission"
              }
            }
          }
        }
      }
    },
    "/submission/count": {
      "get": {
        "tags": [
          "submission"
        ],
        "summary": "get the count of all submissions available for a specific survey",
        "parameters": [
          {
            "in": "query",
            "name": "survey_id",
            "required": true,
            "type": "string"
          },
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "count was calculated",
            "schema": {
              "$ref": "#/definitions/Count"
            }
          }
        }
      }
    },
    "/token": {
      "post": {
        "tags": [
          "token"
        ],
        "summary": "create and retrieve a specific number of token, in a link they can be used to provide access to secure surveys",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "schema": {
              "$ref": "#/definitions/TokenRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "token for links to share a survey",
            "schema": {
              "items": {
                "$ref": "#/definitions/ExampleToken"
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "token"
        ],
        "description": "used_date and submission_id are only there if the token was used. If used is not given all token (used and unused are returned)",
        "summary": "get all participation token of a survey",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "type": "string",
            "name": "survey_id",
            "required": true
          },
          {
            "in": "query",
            "type": "boolean",
            "name": "used"
          },
          {
            "in": "query",
            "type": "number",
            "name": "page_number",
            "default": 0
          },
          {
            "in": "query",
            "type": "number",
            "name": "page_size",
            "default": 3
          }
        ],
        "responses": {
          "200": {
            "description": "the participation token were fetched",
            "schema": {
              "items": {
                "$ref": "#/definitions/TokenObject"
              }
            }
          }
        }
      }
    },
    "/token/count": {
      "get": {
        "tags": [
          "token"
        ],
        "summary": "get the count of participation tokens, created for a particular survey",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "survey_id",
            "type": "string",
            "required": true
          },
          {
            "in": "query",
            "type": "boolean",
            "name": "used"
          },
          {
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Count",
            "schema": {
              "$ref": "#/definitions/Count"
            }
          },
          "400": {
            "description": "Count could not be fetched"
          },
          "500": {
            "description": "Something went wrong, emails could not be send"
          }
        }
      }
    },
    "/token/email": {
      "post": {
        "tags": [
          "token"
        ],
        "summary": "send linkt to secured survey per mail",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "payload",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SendTokenPerMailRequest"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "links were sent per mail to emails in body"
          },
          "403": {
            "description": "User is not authorized to send mails"
          },
          "500": {
            "description": "Something went wrong, emails could not be send"
          }
        }
      }
    },
    "/token/{token_id}": {
      "delete": {
        "tags": [
          "token"
        ],
        "summary": "delete unused token",
        "description": "Only unused token can be deleted",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "in": "path",
            "name": "token_id",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "participation token was deleted"
          },
          "400": {
            "description": "participation token could not be delted"
          },
          "403": {
            "description": "forbidden, authorization is missing"
          }
        }
      }
    }
  },
  "definitions": {
    "ParticipationToken": {
      "type": "array",
      "items": {
        "example": "slkjd-flkjs-fd345"
      }
    },
    "ChangeUserData": {
      "type": "object",
      "required": [
        "username",
        "email",
        "old_password",
        "new_password"
      ],
      "properties": {
        "username": {
          "type": "string",
          "example": "bongoCat"
        },
        "email": {
          "type": "string",
          "example": "bongo@cat.de"
        },
        "old_password": {
          "type": "string",
          "example": "whatTheUserThinksIsOldPassword"
        },
        "new_password": {
          "type": "string",
          "example": "newStrongPassword"
        }
      }
    },
    "ResetPasswordRequest": {
      "type": "object",
      "required": [
        "username",
        "email"
      ],
      "properties": {
        "username": {
          "type": "string",
          "example": "admin"
        },
        "email": {
          "type": "string",
          "example": "admin@mail.de"
        }
      }
    },
    "ResetPassword": {
      "type": "object",
      "required": [
        "reset_password_token",
        "new_password"
      ],
      "properties": {
        "reset_password_token": {
          "type": "string",
          "example": "fgHd-73hg-83HZ"
        },
        "new_password": {
          "type": "string",
          "example": "newstrongpassword"
        }
      }
    },
    "SendTokenPerMailRequest": {
      "type": "object",
      "required": [
        "survey_id",
        "emails"
      ],
      "properties": {
        "survey_id": {
          "type": "string",
          "example": "fgHd-73hg-83HZ"
        },
        "emails": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Mail"
          }
        }
      }
    },
    "Mail": {
      "type": "string",
      "example": "maxmusterman@mail.de"
    },
    "TokenRequest": {
      "type": "object",
      "required": [
        "survey_id",
        "amount"
      ],
      "properties": {
        "survey_id": {
          "type": "string",
          "example": "fgHd-73hg-83HZ"
        },
        "amount": {
          "type": "number",
          "example": 30
        }
      }
    },
    "User": {
      "type": "object",
      "required": [
        "id",
        "username",
        "email",
        "created"
      ],
      "properties": {
        "id": {
          "type": "string",
          "example": "fgHd-73hg-83HZ"
        },
        "username": {
          "type": "string",
          "example": "maxmusterman"
        },
        "email": {
          "type": "string",
          "example": "max@musterman.de"
        },
        "created": {
          "type": "string",
          "example": "25.09.2020"
        }
      }
    },
    "DeleteUser": {
      "type": "object",
      "required": [
        "password"
      ],
      "properties": {
        "password": {
          "type": "string",
          "example": "superstrongpassword"
        }
      }
    },
    "Count": {
      "type": "object",
      "required": [
        "count"
      ],
      "properties": {
        "count": {
          "type": "number",
          "example": 12
        }
      }
    },
    "ExampleToken": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "example": "exampleToken"
        }
